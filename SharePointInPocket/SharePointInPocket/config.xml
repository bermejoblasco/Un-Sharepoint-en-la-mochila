<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="sogeti.spsmadrid.sharepointmochila" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" defaultlocale="en-US" windows-packageVersion="1.1.0.0">
  <!--<widget id="sogeti.spsmadrid.sharepointmochila" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">-->
  <name>SharePointMochilero</name>
  <description>Demo para el SPSMadrid: Un Sharepoint en tu mochila</description>
  <author email="you@example.com" href="http://example.com.com/">
Sogeti España,SLU
    </author>
  <content src="index.html" />
  <access origin="*" />
  <vs:features />
  <preference name="webviewbounce" value="false" />
  <preference name="UIWebViewBounce" value="false" />
  <preference name="DisallowOverscroll" value="true" />
  <preference name="android-minSdkVersion" value="16" />
  <preference name="BackupWebStorage" value="none" />
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar" />
  </feature>
  <preference name="KeepRunning" value="True" />
  <preference name="ShowTitle" value="True" />
  <preference name="InAppBrowserStorageEnabled" value="True" />
  <preference name="SuppressesIncrementalRendering" value="True" />
  <preference name="windows-target-version" value="10.0" />
  <plugin name="cordova-plugin-device" spec="~1.1.1" />
  <plugin name="cordova-plugin-console" spec="~1.0.2" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <allow-navigation href="*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <plugin name="cordova-plugin-statusbar" spec="~2.1.0" />
  <plugin name="ionic-plugin-keyboard" spec="~1.0.8" />
  <preference name="Fullscreen" value="True" />
  <plugin name="cordova-plugin-file-transfer" version="1.5.2-dev" src="https://github.com/apache/cordova-plugin-file-transfer.git" />
  <plugin name="cordova-plugin-file" version="4.2.0" src="https://github.com/apache/cordova-plugin-file.git" />
  <plugin name="cordova-plugin-compat" version="1.0.0" />
  <plugin name="cordova-plugin-file-opener2" version="2.0.2" src="https://github.com/pwlin/cordova-plugin-file-opener2.git" />
  <plugin name="cordova-plugin-ms-azure-mobile-apps" version="2.0.0-beta4" />
  <plugin name="cordova-plugin-inappbrowser" version="1.4.0" />
  <plugin name="phonegap-plugin-push" version="1.6.3" src="https://github.com/phonegap/phonegap-plugin-push.git">
    <param name="SENDER_ID" value="111111111111" />
  </plugin>
  <plugin name="cordova-plugin-whitelist" version="1.2.3-dev" src="https://github.com/apache/cordova-plugin-whitelist.git" />
  <vs:platformSpecificValues>
    <vs:platformSpecificWidget platformName="windows" version="1.0.0.0" id="14151RobertBermejo.SharepointMochilero">
      <vs:appxPackageIsForStore>True</vs:appxPackageIsForStore>
    </vs:platformSpecificWidget>
  </vs:platformSpecificValues>
  <preference name="WindowsStoreDisplayName" value="Sharepoint Mochilero" />
  <preference name="WindowsStoreIdentityName" value="14151RobertBermejo.SharepointMochilero" />
  <preference name="WindowsStorePublisherName" value="Robert Bermejo" />
</widget>